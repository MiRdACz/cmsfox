<!DOCTYPE html>
<html lang="cs">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width">
    <meta name="description" content="{ifset description}{include description}{else}{include title}{/ifset}" />
    <meta name="keywords" content="{include title}" />
	<title>{ifset title}{include title|stripHtml} | {/ifset}Nette Web</title>
	{switch $url}
      {case admin}
      <script n:nonce src="{$baseUrl}/js/jquery.min.js"></script>
      <link n:nonce rel="stylesheet" href="{$baseUrl}/js/jquery-ui-1.12.1/jquery-ui.min.css" />
      <script n:nonce src="{$baseUrl}/js/jquery-ui-1.12.1/jquery-ui.min.js"></script>
    {/switch}
	<link n:nonce href="{$baseUrl}/css/style.css" rel="stylesheet">
	<link n:nonce rel="stylesheet" href="{$baseUrl}/css/aos.css" />
	<link n:nonce href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" rel="stylesheet">
	<link n:nonce href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
</head>
<body>
    {switch $url}
        {case admin}
        {include 'adminMenu.latte'}
        {default}
        {include 'menu.latte'}
    {/switch}
	<div n:foreach="$flashes as $flash" n:class="mb-0,alert,alert-dismissible,fade,show,text-center,$flash->type" role="alert">{$flash->message|noescape}<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button></div>
	{include content}
	{switch $url}
      {case admin}
      {default}
        {include 'PhotoSwipe.latte'}
    {/switch}
    <script n:nonce src="{$baseUrl}/js/Naja.js"></script>
	<script n:nonce src="{$baseUrl}/js/main.js"></script>
	<script n:nonce src="https://unpkg.com/aos@next/dist/aos.js"></script>
	<script n:nonce src="https://cdn.jsdelivr.net/npm/lozad/dist/lozad.min.js"></script>
	<script n:nonce src="{$baseUrl}/js/lozad.js"></script>
	<script n:nonce src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js" integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous"></script>
	<script n:nonce src="https://nette.github.io/resources/js/3/netteForms.min.js"></script>
	{block script}{/block}
</body>
</html>
